{
  "/api/voice/*": {
    "target": "http://localhost:5001",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "headers": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
      "Access-Control-Allow-Headers": "*"
    }
  },
  "/api/game-world/*": {
    "target": "http://localhost:5003",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug"
  },
  "/api/talking/*": {
    "target": "http://localhost:5002",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug"
  }
} 